@article{andrews_age_1999,
  title = {Age and Growth of the {{Pacific Grenadier}} ({{{\emph{Coryphaenoides}}}}{\emph{ Acrolepis}}) with Age Estimate Validation Using an Improved Radiometric Ageing Technique},

@article{breeggemann_estimating_2014,
  title = {Estimating the Ages of {{Mountain Sucker}} {{{\emph{Catostomus}}}}{\emph{ Platyrhynchus}} from the {{Black Hills}}: {{Precision}}, Maturation, and Growth},

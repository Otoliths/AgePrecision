@article{lombardi-carlson_regional_2008,
  title = {Regional Size, Age and Growth Differences of {{Red Grouper}} ({{{\emph{Epinephelus}}}}{\emph{ Morio}}) along the West Coast of {{Florida}}},
  volume = {91},
  issn = {0165-7836},
  doi = {10.1016/j.fishres.2007.12.001},
  abstract = {Red grouper (Epinephelus morio) were collected from the west coast of Florida, the central area of fishery harvest in U.S. waters, by fishery-dependent sources during 2000\textendash{}2005. The west Florida shelf was divided into two regions: north (capture locations $\geq$28$^\circ$N latitude) and south (capture locations $<$28$^\circ$N latitude). Significant differences were found for age, length, and size-at-age by region and by gear; red grouper from the north were significantly younger and smaller on average than those from the south. Regional differences were also noted with respect to age progression; year class trends were only detected in the north. The 1996 year class dominated the landings in 2000\textendash{}2001 (ages 4 and 5) and the 1999 year class dominated in 2004\textendash{}2005 (ages 5 and 6). Regional data were fit to a size-modified von Bertalanffy growth model indicating smaller asymptotic length (L$\infty$) and faster growth rate (k) in the north (north: L$\infty$=800mm, k=0.23mmyear-1, t0=1.12; south: L$\infty$=863mm, k=0.15mmyear-1, t0=0.05). Mortality estimates derived from catch curves resulted in higher total and fishing mortality in the north for both gears. Alternative explanations of regional differences likely depend on nursery delineation and correlation to periodic environmental events such as red tides and hurricanes; all possibly important factors based upon anecdotal information. Nevertheless, our finding of regional demographic differences in red grouper from the west coast of Florida suggests a more complex population spatial structure for red grouper.},
  journal = {Fisheries Research},
  author = {{Lombardi-Carlson}, Linda and Fitzhugh, Gary and Palmer, Chris and Gardner, Chris and Farsky, Robert and Ortiz, Mauricio},
  month = jun,
  year = {2008},
  keywords = {Life history,Red grouper,Spatial and temporal scales,Stock},
  pages = {239-251},
  file = {C:\\Users\\dogle\\Zotero\\storage\\7BI3FSH6\\Lombardi-Carlson et al. - 2008 - Regional size, age and growth differences of red g.pdf}
}


@article{schrank_age_2002,
  title = {Age, Growth, and Gonadal Characteristics of Adult {{Bighead Carp}}, {{{\emph{Hypophthalmichthys}}}}{\emph{ Nobilis}}, in the {{Lower Missouri River}}},
